{
	"manifest_version": 3,
	"name": "Favnekon3 Maker",
	"description": "__MSG_appDesc__",
	"version": "3.2",
	"default_locale": "en_US",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
	  "default_icon": "^향^.png",
	  "default_popup": "popup_db.html"
	},
