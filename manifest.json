		"resources": ["tools/*", "content_styles.css"],
		"matches": ["<all_urls>"]
	}],
	"host_permissions": ["<all_urls>"],
	"permissions": [
		"contextMenus",
		"scripting",
		"tabs"
	],
	"icons": {
		"16" : "^향^.png",
		"48" : "^향^.png",
		"128": "^향^.png"
	}
}
